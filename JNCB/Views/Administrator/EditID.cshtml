@model Teller

<h1>Edit Customer Details</h1>


<div class="card-header">
    <ul class="nav nav-tabs card-header-tabs">
        <li class="nav-item">
            <a class="nav-link " asp-action="EditT" asp-route-id="@Model.id">Teller Info</a>
        </li>

        <li class="nav-item">
            <a class="nav-link active" href="#">ID</a>
        </li>
    </ul>
</div>
<div class="card-body">
    <div class="row">
        <div class="col-md-4">
            <form asp-action="EditID">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="empid" />

                <div class="form-group">
                    <label asp-for="empid" class="control-label">Account Number</label>
                    <input asp-for="empid" class="form-control" disabled />
                    <span asp-validation-for="empid" class="text-danger"></span>
                </div>



                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>

            </form>
        </div>
    </div>
</div>


<div>
    <a asp-action="ViewAllTeller">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}